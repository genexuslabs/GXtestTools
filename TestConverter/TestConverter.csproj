<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>GeneXus.GXtest.Tools.TestConverter</RootNamespace>
    <Description>Helps converting tests from GXtest v3 to v4</Description>
    <PackageProjectUrl>https://github.com/genexuslabs/gxtest-tools#readme</PackageProjectUrl>
    <RepositoryUrl>https://github.com/genexuslabs/gxtest-tools</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\TestConvertBL\TestConvert.BL.csproj" />
  </ItemGroup>

</Project>
